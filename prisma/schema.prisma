datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Project {
  id        String   @id @default(uuid())
  code      String
  settings  Json
  type      String
  password  String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
